{
    "folder": "resources/views",
    "file_types": [
        "blade.php",
        ".jsx",
        ".vue"
    ],
    "ignore_names": [
        "someFolderNameToIgnore",
        "images"
    ],
    "ignore": ["%", "#", "_", ">", "{", "(", "}", ")", "^", "$", "*", "=", "'"],
    "methods": [
        "text",
        "placeholder",
        "alt",
        "title",
        "hashtag"
    ],
    "prefix": "{{ __('",
    "suffix": "') }}",
    "detailed": false,
    "force": false,
    "log_folder": "hsa/log"
}